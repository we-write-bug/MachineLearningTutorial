<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.example.papersleep.machinelearningtutorial">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning,UnusedAttribute">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".tutorial.TutorialActivity">
        </activity>
        <activity android:name=".practice.PracticeActivity"
                  android:launchMode="singleTask">
        </activity>
        <activity android:name=".not_activity.BaseActivity"
                  android:launchMode="singleTask">
        </activity>
        <activity android:name=".practice.EnvSetting"
                  android:launchMode="singleTask">
        </activity>
        <activity android:name=".practice.FuncSetting"
                  android:launchMode="singleTask">
        </activity>
        <activity android:name=".practice.Evolution"
                  android:launchMode="singleTask">
        </activity>
        <activity android:name=".practice.FightIt"
                  android:launchMode="singleTask">
        </activity>
        <activity android:name=".practice.EndingAndSearching"
                  android:launchMode="singleTask">
        </activity>
    </application>

</manifest>